   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "from sklearn.impute import SimpleImputer\n",
    "\n",
    "# 1. Simulando uma matriz de avaliações fictícia\n",
    "data = {\n",
    "    'user_id': [1, 1, 2, 2, 3, 3, 4, 4],\n",
    "    'product_id': [101, 102, 101, 103, 102, 104, 103, 104],\n",
    "    'rating': [5, 3, 4, 2, 5, 4, 3, 5]\n",
    "}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# 2. Criar matriz usuário-produto\n",
    "user_item_matrix = df.pivot_table(index='user_id', columns='product_id', values='rating')\n",
    "\n",
    "# Preenchendo valores NaN com a média\n",
    "imputer = SimpleImputer(strategy='mean')\n",
    "user_item_matrix_filled = imputer.fit_transform(user_item_matrix)\n",
    "\n",
    "# 3. Calcular similaridade entre usuários\n",
    "user_similarity = cosine_similarity(user_item_matrix_filled)\n",
    "user_similarity_df = pd.DataFrame(user_similarity, index=user_item_matrix.index, columns=user_item_matrix.index)\n",
    "\n",
    "# 4. Visualizar similaridade entre usuários\n",
    "plt.figure(figsize=(8,6))\n",
    "sns.heatmap(user_similarity_df, annot=True, cmap='coolwarm', fmt='.2f')\n",
    "plt.title(\"Mapa de Calor da Similaridade entre Usuários\")\n",
    "plt.xlabel(\"Usuário\")\n",
    "plt.ylabel(\"Usuário\")\n",
    "plt.show()\n",
    "\n",
    "# 5. Função para recomendar produtos\n",
    "def recommend_products(user_id, top_n=3):\n",
    "    user_idx = user_id - 1\n",
    "    similar_users = user_similarity[user_idx]\n",
    "    scores = np.dot(similar_users, user_item_matrix_filled)\n",
    "    user_rated = user_item_matrix.loc[user_id].dropna().index\n",
    "    recommendations = pd.Series(scores, index=user_item_matrix.columns).drop(user_rated, errors=\"ignore\")\n",
    "    return recommendations.sort_values(ascending=False).head(top_n)\n",
    "\n",
    "# 6. Gerar recomendações para um usuário\n",
    "user_id = 1\n",
    "recommended_products = recommend_products(user_id)\n",
    "\n",
    "# 7. Criar gráfico de barras para recomendações\n",
    "plt.figure(figsize=(8,5))\n",
    "sns.barplot(x=recommended_products.values, y=recommended_products.index, palette='viridis')\n",
    "plt.xlabel(\"Pontuação da Recomendação\")\n",
    "plt.ylabel(\"ID do Produto\")\n",
    "plt.title(f\"Top {len(recommended_products)} Recomendações para o Usuário {user_id}\")\n",
    "plt.show()"
   ]
  },
